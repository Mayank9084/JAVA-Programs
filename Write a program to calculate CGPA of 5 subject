import java.util.Scanner;
public class CGPA_Finder {
    public static void main(String[] agrs){
    int sub1, sub2, sub3, sub4, sub5, total;
    float cgpa;
    Scanner sc = new Scanner(System.in);
    // * Grade point of Subject 1 * //
        System.out.println("Enter Grade Points of Subject 1 = ");
        sub1 = sc.nextInt();
        System.out.println(sub1);
        // * Grade point of Subject 2 * //
        System.out.println("Enter Grade Points of Subject 2 = ");
        sub2 = sc.nextInt();
        System.out.println(sub2);
        // * Grade point of Subject 3 * //
        System.out.println("Enter Grade Points of Subject 3 = ");
        sub3 = sc.nextInt();
        System.out.println(sub3);
        // * Grade point of Subject 4 * //
        System.out.println("Enter Grade Points of Subject 4 = ");
        sub4 = sc.nextInt();
        System.out.println(sub4);
        // * Grade point of Subject 5 * //
        System.out.println("Enter Grade Points of Subject 5 = ");
        sub5 = sc.nextInt();
        System.out.println(sub5);
        // * Total Grade points * //
        total = sub1 + sub2 + sub3 + sub4 + sub5;
        System.out.println("Total Grade Points = " +total);
        // * overall CGPA  condition * //
        cgpa = total / 5;
        System.out.println("CGPA is " +cgpa);
    }
}


output :- 

Enter Grade Points of Subject 1 = 
9
9
Enter Grade Points of Subject 2 = 
8
8
Enter Grade Points of Subject 3 = 
7
7
Enter Grade Points of Subject 4 = 
8
8
Enter Grade Points of Subject 5 = 
8
8
Total Grade Points = 40
CGPA is 8.0
