import java.util.Scanner;
public class Natural_no_Rev_ord {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println("The reverse order is: ");
        for(int i=n-1; i>=0; i--){
            System.out.println(i);
        }
        }
    }
