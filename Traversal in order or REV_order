public class Arr {
    public static void main (String[] args){

        // Traversal in order


      //  int[] marks={11,12,13,14,15};
      //  for(int i=0;i<marks.length;i++){
       //     System.out.println(marks[i]);

        // traversal in reverse order

            int[] marks={11,12,13,14,15};
            for(int i= marks.length-1;i>=0;i--){
                System.out.println(marks[i]);
        }
    }
}
